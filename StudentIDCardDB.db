CREATE DATABASE StudentIDCardDB;

USE StudentIDCardDB;

CREATE TABLE students (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(100),
    contact VARCHAR(20),
    branch VARCHAR(50),
    usn VARCHAR(50),
    address VARCHAR(255),
    parent_name VARCHAR(100),
    gmail VARCHAR(100),
    blood_group VARCHAR(5),
    joining_year VARCHAR(4),
    photo_path VARCHAR(255)
);
